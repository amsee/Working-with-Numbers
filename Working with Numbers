# Working with Numbers
from math import *

# Print out an integer (whole number)
print(1)

# Print out a float (decimal)
print(-2.9348)

my_num = 5
print(my_num)

# Convert an integer into a string
print(str(my_num))

# Convert int -> str and concatenate with another str
print(str(my_num) + " is my favourite number") # cannot concat with different datatypes

# Get the absolute value of a number
my_num = -5
print(abs(my_num))

# Get the power
print(pow(4, 6))

# Get the larger number
print(max(5, 10))

# Get the smaller number
print(min(2,4,6))

# Round number to the closest whole number
print(round(3.4))
print(round(3.5))

# Using from math import *
print(floor(3.7))
print(ceil(3.7))
print(sqrt(36))
